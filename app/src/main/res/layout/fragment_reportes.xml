            android:textColor="#F2F1EA"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/txtEntrenador3"
            app:layout_constraintTop_toBottomOf="@+id/txtEntrenador3" />

        <TextView
            android:id="@+id/txtMes3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Noviembre"
            android:textColor="#F2F1EA"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/txtFechaEval3"
            app:layout_constraintTop_toTopOf="@+id/txtFechaEval3" />

        <RatingBar
            android:id="@+id/ratingBar3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:isIndicator="true"
            android:rating="3"
            android:stepSize="1"
            android:theme="@style/RatingBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtFechaEval3" />

        <TextView
            android:id="@+id/txtNote3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Metrica de x clases*"
            android:textColor="#F2F1EA"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="@+id/ratingBar3"
            app:layout_constraintStart_toStartOf="@+id/ratingBar3"
            app:layout_constraintTop_toBottomOf="@+id/ratingBar3" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>

</ScrollView>