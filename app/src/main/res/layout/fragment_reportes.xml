            android:textColor="#F2F1EA"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/txtEntrenador2"
            app:layout_constraintTop_toTopOf="@+id/txtEntrenador2" />

        <TextView
            android:id="@+id/txtFechaEval2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/lbEval"
            android:textColor="#F2F1EA"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/txtEntrenador2"
            app:layout_constraintTop_toBottomOf="@+id/txtEntrenador2" />

        <TextView
            android:id="@+id/txtMes2"
            android:layout_width="wrap_content"
